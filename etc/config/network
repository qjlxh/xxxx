
config interface 'loopback'
	option ifname 'lo'
	option proto 'static'
	option ipaddr '127.0.0.1'
	option netmask '255.0.0.0'

config globals 'globals'
	option packet_steering '1'

config interface 'lan'
	option ifname 'eth0'
	option proto 'static'
	option netmask '255.255.255.0'
	option ip6assign '60'
	option delegate '0'
	option ipaddr '192.168.1.1'
	option gateway '192.168.1.86'
	option dns '192.168.1.1'

config interface 'wan'
	option proto 'pppoe'
	option ifname 'eth0'
	option username ''
	option password ''
	option ipv6 'auto'
	option keepalive '0'

config interface 'utun'
	option proto 'none'
	option ifname 'utun'
	option device 'utun'


